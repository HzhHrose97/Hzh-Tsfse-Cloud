<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzh.common.mapper.mq.AsyncMessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hzh.common.pojo.centre.AsyncMessage">
        <id column="message_id" property="messageId" />
        <result column="topic" property="topic" />
        <result column="message_body" property="messageBody" />
        <result column="created_at" property="createdAt" />
        <result column="status" property="status" />
        <result column="consumed" property="consumed" />
        <result column="producer" property="producer" />
        <result column="publisher_id" property="publisherId" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        message_id, topic, message_body, created_at, status, consumed, producer, publisher_id, remark
    </sql>

</mapper>
